{
  "name": "react-sk",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "elegant-standard": "^2.0.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "mobx-state-tree": "^3.14.1",
    "mst-persist": "^0.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-regex-router": "^1.0.0",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:css start:react",
    "build": "npm-run-all build:css build:react",
    "build:css": "postcss src/global/tailwind.css -o src/global/tailwind.out.css",
    "watch:css": "postcss src/global/tailwind.css -o src/global/tailwind.out.css -w",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.0.4"
  }
}
